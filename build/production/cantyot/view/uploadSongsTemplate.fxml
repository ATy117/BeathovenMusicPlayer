<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="435.0" prefWidth="546.0" style="-fx-background-color: #454444" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="addEditSong" fill="#eaa830" layoutX="227.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Song">
         <font>
            <Font name="Comfortaa Regular" size="18.0" />
         </font>
      </Text>
      <JFXTextField fx:id="titleField" focusColor="#e9c582" layoutX="132.0" layoutY="119.0" prefHeight="30.0" prefWidth="282.0" promptText="Title" unFocusColor="#eaa830">
         <font>
            <Font name="Poppins Regular" size="14.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="artistField" focusColor="#e9c582" layoutX="133.0" layoutY="162.0" prefHeight="30.0" prefWidth="282.0" promptText="Artist" unFocusColor="#eaa830">
         <font>
            <Font name="Poppins Regular" size="14.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="albumField" focusColor="#e9c582" layoutX="134.0" layoutY="203.0" prefHeight="30.0" prefWidth="282.0" promptText="Album" unFocusColor="#eaa830">
         <font>
            <Font name="Poppins Regular" size="14.0" />
         </font>
      </JFXTextField>
      <JFXComboBox fx:id="genreField" focusColor="#e9c582" layoutX="191.0" layoutY="251.0" prefHeight="25.0" prefWidth="164.0" promptText="Genre" unFocusColor="#eaa830" />
      <JFXButton fx:id="uploadBtn" layoutX="193.0" layoutY="297.0" onAction="#uploadSongFile" prefHeight="30.0" prefWidth="164.0" style="-fx-background-color: #303030" text="Upload" textFill="#8ba0a9">
         <font>
            <Font name="Poppins Regular" size="14.0" />
         </font>
      </JFXButton>
      <JFXTextField fx:id="fileField" focusColor="#e9c582" layoutX="175.0" layoutY="327.0" prefHeight="30.0" prefWidth="216.0" promptText="File" unFocusColor="#eaa830">
         <font>
            <Font name="Poppins Regular" size="14.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="doneBtn" layoutX="255.0" layoutY="374.0" onAction="#addEditSongDetails" style="-fx-background-color: #303030" text="Done" textFill="#8ba0a9">
         <font>
            <Font name="Poppins Regular" size="14.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
